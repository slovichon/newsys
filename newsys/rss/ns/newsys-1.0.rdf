<!DOCTYPE rdf:RDF
[
	<!ENTITY rdfns		'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	<!ENTITY rdfsns		'http://www.w3.org/2000/01/rdf-schema#'>
	<!ENTITY newsysns	'http://www.easyphp.net/projects/newsys/pub/newsys-ns/newsys-1.0-schema#'>
]
>

<rdf:RDF xmlns:rdf="&rdfns;"
	 xmlns:rdfs="&rdfsns;"
	 xmlns:newsys="&newsysns;">

	<rdf:Property rdf:about="&newsysns;timestamp">
		<rdfs:label>Timestamp</rdfs:label>
		<rdfs:comment>Timestamp for an story in standard UNIX format</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="&newsysns;" />
	</rdf:Property>
</rdf:RDF>
